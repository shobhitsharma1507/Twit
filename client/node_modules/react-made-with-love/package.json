{
  "name": "react-made-with-love",
  "version": "1.0.1",
  "description": "A tiny React component providing a 'Made With Love' banner that can be customised with emojis and SVG icons.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src --out-file lib/index.js --ignore test.js",
    "build:docs": "webpack",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "component",
    "UI"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisgreg/react-made-with-love.git"
  },
  "bugs": {
    "url": "https://github.com/chrisgreg/react-made-with-love/issues"
  },
  "author": "Chris Gregori <chrisgreg23@googlemail.com>",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.2",
    "react-inlinesvg": "^0.5.5"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.7.1",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-element-to-jsx-string": "^6.0.0",
    "webpack": "^2.2.1"
  }
}
